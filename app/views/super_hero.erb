<h1>Create a Team and Heroes!</h1>
<form method="post" action="/teams">
  <p> Team Name: <input type="text" name="team[name]"></p>
  <p> Team Motto: <input type="text" name="team[motto]"></p>

  <label> Member 1 </label>
  <p> Team Member 1 Name : <input id="member1_name" type="text" name="team[member][][name]"></p>
  <p>Team Member 1 Power : <input id="member1_power" type="text" name="team[member][][power]"></p>
  <p>Team Member 1 Bio : <input id="member1_bio" type="text" name="team[member][][bio]"></p>

  <label> Member 2 </label>
  <p> Team Member 2 Name: <input id="member2_name" type="text" name="team[member][][name]"></p>
  <p>Team Member 2 Power : <input id="member2_power" type="text" name="team[member][][power]"></p>
  <p>Team Member 2 Bio : <input id="member2_bio" type="text" name="team[member][][bio]"></p>

<label> Member 3 </label>
  <p> Team Member 3 Name: <input id="member3_name" type="text" name="team[member][][name]"></p>
  <p>Team Member 3 Power : <input id="member3_power" type="text" name="team[member][][power]"></p>
  <p>Team Member 3 Bio : <input id="member3_bio" type="text" name="team[member][][bio]"></p>

  <input type="submit" id = "submit" value="submit">

</form>
